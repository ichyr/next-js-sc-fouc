{
    "env": {
      "development": {
        "presets": ["next/babel"],
        "plugins": [
          [
            "babel-plugin-styled-components",
            {
              "ssr": true,
              "displayName": true,
              "preprocess": false
            }
          ]
        ]
      },
      "production": {
        "presets": "next/babel",
        "plugins": [
          [
            "babel-plugin-styled-components",
            {
              "displayName": false,
              "ssr": true,
              "preprocess": false
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ],
          "next/babel"
        ]
      }
    }
  }
  